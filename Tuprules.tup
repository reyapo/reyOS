export reyOS
export BUILD_ROOT
export PREFIX
export PATH

include config

CC  = "$(TARGET)-elf-gcc"
CPP = "$(TARGET)-elf-g++"
AR  = "$(TARGET)-elf-ar"

ARCH=$(TARGET)

ifeq ($(TARGET),i686)
ARCH=i386
endif

!cpp  = |> ^ CPP %o^ $(CPP) |>
!cc   = |> ^ CC %o^ $(CC) |>
!nasm = |> ^ nasm %o^ nasm |>
!ar   = |> ^ AR %o^ $(AR) |>

